{
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./local/assets/bell.png",
          "color": "#ffffff",
          "sounds": ["./local/assets/bell.wav"]
        }
      ]
    ],
    "name": "be-social",
    "userInterfaceStyle": "automatic",
    "slug": "be-social",
    "version": "1.0.0",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "y"
    },
    "android": {
      "package": "com.BeSocial.besocial"
    }
  },
  "name": "be-social"
}
